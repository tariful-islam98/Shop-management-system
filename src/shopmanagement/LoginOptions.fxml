<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER" spacing="40.0" style="-fx-background-color: #CFCCDF;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shopmanagement.LoginOptionsController">
   <children>
      <Text fill="#790000" strokeType="OUTSIDE" strokeWidth="0.0" text="Select an Option">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <Button mnemonicParsing="false" onAction="#employeeLoginBtnAction" prefHeight="44.0" prefWidth="170.0" style="-fx-background-color: #566EB1;" text="Employee Login" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#adminLoginBtnAction" prefHeight="44.0" prefWidth="170.0" style="-fx-background-color: #566EB1;" text="Admin Login" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</VBox>
